<% var myId, partnerId, partnerName; %>

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta content="text/html;charset=utf-8" http-equiv="Content-Type">
    <meta content="utf-8" http-equiv="encoding">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
    <script src="https://rawgit.com/mburakerman/hrjs/master/src/hr.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/mark.js/8.11.1/mark.min.js"></script>
   <script src="https://johannburkard.de/resources/Johann/jquery.highlight-5.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/highlight.js/9.18.3/highlight.min.js"></script>



    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <script src="../scripts/play.js"></script>

    <link href="https://fonts.googleapis.com/css2?family=Nunito+Sans:ital,wght@0,300;0,400;0,800;1,900&display=swap"
          rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@splidejs/splide@latest/dist/css/splide.min.css">
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.27.0/moment.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.3.0/socket.io.js"></script>
    <meta content="text/html;charset=utf-8" http-equiv="Content-Type">
    <link rel="stylesheet" href="../css/style.css">


</head>
<title>IT-Labs</title>

<body onload="play()">
<h1 data-chronometer class="cronometru">00:00:00</h1>


<nav class="nav">

    <a href="#" class="nav__logo">
        <img title="IT-Labs" src="../img/logo.png" alt="IT-Labs Logo">
    </a>
    <ul class="nav__menu">
        <li class="nav__item">
            <button id="closeSessionBtn"><img src="../img/icons8-close-window-26.png"/>
            </button>
        </li>
    </ul>
</nav>
<div class="container_sess">

    <div class="div_session_nav u-flex-column">
        <nav class="session_nav">
            <button class="sess_button" id="codeButton" > CODE EDITOR</button>
            <button class="sess_button" id="audioVideoButton"> AUDIO/VIDEO</button>
            <button class="sess_button" id="whiteboardButton"> WHITEBOARD</button>
        </nav>
        <div id="option"></div>
    </div>


    <div class="div_session_chat">
        <form action="#" class="form-container u-flex-column" style="width: 100%; height: 61rem">
            <div class="chat_header color-section--primary"></div>
            <div id="chatProfile" class="u-flex">
                <div><span id="profile_picture"></span><span id="profileName"
                                                             class="profile_name"><%= partnerName %></span>
                </div>
            </div>
            <div id="messagesContainer" class="u-flex-column"></div>
            <div class="send_message_container">
                <textarea id="messageText" placeholder="Type message.." name="msg"></textarea>

                <button id="sendMessageBtn" class="btn">Send</button>
            </div>
        </form>

    </div>
    <p id="myId" style="display: none"><%= myId %></p>
    <p id="partnerId" style="display: none"><%= partnerId %></p>
</div>
<footer class="footer">
    <div class="row">

        <div class="grid">
            <div class="grid__3">
                <img src="../img/logo_white.png" alt="IT-Labs white and black logo" class="footer__logo">
            </div>
            <div class="grid__9 footer__heading">
                IT LABS este o comunitate si un ansamblu de resurse pentru devenirea ta ca programator.
            </div>
        </div>
    </div>
</footer>
<script src="../scripts/bundle.js"></script>
<script src="../scripts/session.js" type="text/javascript"></script>
<script src="../scripts/chatCommon.js"></script>
<script src="../scripts/chatSession.js"></script>
<script src="../scripts/whiteboard.js"></script>
<script src="../scripts/livecoding.js"></script>
<script type="text/javascript" src="../scripts/python.json"></script>
<script src="../scripts/audioVideoSession.js"></script>
<script src="../scripts/sessionButtonsEvents.js"></script>
</body>